	Keywords = {galaxies: evolution, galaxies: statistics, galaxies: stellar content},
	Mnrascitekey = Asari.etal.2007,
	Month = oct,
	Pages = {263-279},
	Title = {{The history of star-forming galaxies in the Sloan Digital Sky Survey}},
	Volume = 381,
	Year = 2007,
