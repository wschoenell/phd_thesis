	Author = {{Denus-Baillargeon}, Marie-Maude~de and {Boissier}, Samuel and {Amram}, Philippe and {Carignan}, Claude and {Hernandez}, Olivier},
	Date-Added = {2013-07-11 02:46:02 +0000},
	Date-Modified = {2013-08-30 16:59:06 +0000},
	Eprint = {1307.2516v1},
	Jornal = {ArXiv e-prints},
	Month = {Jul},
	Primaryclass = {astro-ph.CO},
	Title = {A galactic weigh-in: mass models of SINGS galaxies using chemospectrophotometric galactic evolution models},
	Year = {2013},
