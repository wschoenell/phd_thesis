	Doi = {10.1086/175332},
	Journal = {\apj},
	Keywords = {COLOR, COSMOLOGY, GALACTIC EVOLUTION, LYMAN SPECTRA, RADIATIVE TRANSFER, RED SHIFT, UNIVERSE, ABSORPTION SPECTRA, BACKGROUND RADIATION, RESONANCE SCATTERING, STOCHASTIC PROCESSES, ULTRAVIOLET ASTRONOMY},
	Mnrascitekey = Madau.1995,
	Month = mar,
	Pages = {18-27},
	Title = {{Radiative transfer in a clumpy universe: The colors of high-redshift galaxies}},
