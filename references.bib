	Author = {{Buat}, V. and {Heinis}, S. and {Boquien}, M. and {Burgarella}, D. and {Charmandaris}, V. and {Boissier}, S. and {Boselli}, A. and {Le Borgne}, D. and {Morrison}, G.},
	Date-Added = {2013-11-08 19:48:24 +0000},
	Date-Modified = {2013-11-08 19:48:24 +0000},
	Eprint = {1310.7712},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Mnrascitekey = Buat.etal.2013,
	Month = oct,
