	Date-Added = {2015-05-20 21:58:08 +0000},
	Date-Modified = {2015-05-20 21:58:09 +0000},
	Doi = {10.1046/j.1365-8711.2001.04366.x},
	Eprint = {astro-ph/0102061},
	Journal = {\mnras},
	Keywords = {GALAXIES: EVOLUTION, GALAXIES: STATISTICS, GALAXIES: STELLAR CONTENT},
	Mnrascitekey = CidFernandes.etal.2001,
