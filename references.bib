	Journal = {\apjl},
	Keywords = {Galaxy: Stellar Content, Stars: Low-Mass, Brown Dwarfs, Stars: Luminosity Function, Mass Function},
	Mnrascitekey = Chabrier.2003,
	Month = apr,
	Pages = {L133-L136},
