	Keywords = {STARS: FORMATION, GALAXIES: EVOLUTION, INTERGALACTIC MEDIUM, QUASARS: ABSORPTION LINES, COSMOLOGY: OBSERVATIONS, ULTRAVIOLET: GALAXIES},
