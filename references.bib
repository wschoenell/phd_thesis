	Eprint = {1308.2675},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Cosmology and Extragalactic Astrophysics},
	Mnrascitekey = Allison.Dunkley.2013,
	Month = aug,
	Primaryclass = {astro-ph.IM},
	Title = {{Comparison of sampling techniques for Bayesian parameter estimation}},
	Year = 2013,
