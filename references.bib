	Arxivurl = {http://arXiv.org/abs/astro-ph/0504434},
	Author = {{Burgarella}, D. and {Buat}, V. and {Iglesias-P{\'a}ramo}, J.},
	Date-Added = {2014-09-10 22:46:31 +0000},
